{
    "compilerOptions": {
        "target": "es6",
        "module": "commonjs",
        "moduleResolution": "node",
        "sourceMap": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "removeComments": true,
        "declaration": true,
        "noImplicitAny": false,
        "noEmitHelpers": true
    },
    "exclude": [
        "node_modules",
        "typings/main",
        "typings/main.d.ts",
        "custom-client-files"
    ]
}